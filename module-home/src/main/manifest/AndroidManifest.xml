<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hjc.module_home">

    <!--    单独module    -->

    <application
        android:name=".HomeApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/home_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/BaseAppTheme">

        <activity android:name=".ui.HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


        <activity
            android:name=".ui.jetpack.JetpackActivity"
            android:screenOrientation="portrait" />

        <activity
            android:name=".ui.jetpack.child.JetDataActivity"
            android:screenOrientation="portrait" />

        <activity
            android:name=".ui.jetpack.child.JetLiveActivity"
            android:screenOrientation="portrait" />

        <activity
            android:name=".ui.jetpack.child.JetAdapterActivity"
            android:screenOrientation="portrait" />

        <activity
            android:name=".ui.jetpack.child.JetPagingActivity"
            android:screenOrientation="portrait" />


        <activity
            android:name=".ui.coroutines.CoroutinesActivity"
            android:screenOrientation="portrait" />


        <activity
            android:name=".ui.compose.ComposeTestActivity"
            android:screenOrientation="portrait" />

    </application>

</manifest>